{
    "package": {
        "name": "p2",
        "repo": "modit-update-site",
        "subject": "mypsycho",
        "desc": "Eclipse plugins for modeling by code. (Pushed by Travis-ci from Github)",
        "website_url": "https://github.com/mypsycho/ModIt/",
        "issue_tracker_url": "https://github.com/mypsycho/ModIt/issues",
        "vcs_url": "https://github.com/mypsycho/ModIt.git",
        "github_use_tag_release_notes": false,
        "github_release_notes_file": "RELEASE.txt",
        "licenses": [ "EPL-1.0" ],
        "labels": [ "Model", "xtend", "eclipse", "EMF", "Ecore", "mod-it", "Tests" ],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "${mvn.build.majorVersion}.${mvn.build.minorVersion}", 
        "desc": "Last version available",
        "released": "${mvn.build.date}",
        "vcs_tag": "master",
        "gpgSign": false
    },

    "files": [
        { "includePattern" : "releng/publish/target/site/(.*)", "uploadPattern" : "$1", "matrixParams": { "override": 1 } },
 	   ],
	
    "publish": true
}